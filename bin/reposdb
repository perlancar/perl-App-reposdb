#!perl

# DATE
# VERSION

use 5.010001;
use strict;
use warnings;

use Perinci::CmdLine::Any;

my $prefix = '/App/reposdb/';
Perinci::CmdLine::Any->new(
    url => $prefix,
    subcommands => {
        ls    => {url => "${prefix}list_repos"},
        touch => {url => "${prefix}touch_repo"},
    },
    log => 1,
)->run;

1;
# ABSTRACT:
# PODNAME:

=head1 DESCRIPTION
